<script type="text/javascript">
    $(document).ready(function () {
        $("[id^='logclick']").each(
            function (index) {
                var thisID = $(this).attr("id");
                $(this).click(function () { siUrl(document.location, thisID.split("_")[1]); });
            }
            );
    });
</script>
