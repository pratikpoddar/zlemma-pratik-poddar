 <script type="text/javascript" language="javascript">
    //<![CDATA[
     $(function () {
         $("#ctl00_FooterSearchForm_SearchButton").click(
                function () {
                    var query = $('#ctl00_FooterSearchForm_txtQuery')[0].value;
                    $.ajax({
                        url: "http://academic.research.microsoft.com/Images/logClick.gif?t=13&query=" + query + "&tm=" + new Date(),
                        success: function () {
                        }
                    });
                    $("#divLoadingIcon").show();
                    var xPosition = 0;
                    setInterval(function () {
                        if (xPosition <= 0)
                            xPosition = 160;
                        xPosition = xPosition - 20;
                        $('#divLoadingIcon').css('background-position', xPosition + 'px 0px');
                    }, 125);

                }
            );

            $("#ctl00_FooterSearchForm_advancedsearch").click(function () {
                siUrl(this.location, 'ctl00_FooterSearchForm_advancedsearch');
            });
            $("#ctl00_FooterSearchForm_divSearchBox").click(function () {
                siUrl(this.location, 'ctl00_FooterSearchForm_divSearchBox');
            });
     })
    //]]>
    </script>
