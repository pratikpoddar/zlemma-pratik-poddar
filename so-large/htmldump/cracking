                    $more = $div.find('.js-show-more'),
                    $hidden = $div.find('.js-hidden');

                $more.click(function () {
                    $hidden.show();
                    $more.remove();
                    return false;
                });

            
            });
        </script>
</div>
    </div>

        </div>
    </div>
